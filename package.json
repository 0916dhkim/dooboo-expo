{
  "name": "dooboo",
  "main": "index.js",
  "version": "0.0.0",
  "description": "Expo starter from dooboo-cli.",
  "author": "hyochan <dooboolab@gmail.com>",
  "license": "",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:android": "cd android && ./gradlew assembleRelease",
    "clean": "rm -rf ./dist",
    "dev": "tsc -w & exp start --offline --dev",
    "start": "tsc -w & exp start",
    "start:android": "exp start -a --dev --lan",
    "tsc": "tsc",
    "watch": "tsc -w",
    "start:offline": "tsc -w & exp start --offline",
    "start:reset": "exp start --offline -c --dev",
    "start:exp": "exp start --lan",
    "production": "exp start --no-dev",
    "test": "node node_modules/jest/bin/jest.js -u",
    "lint:validate": "tsc --noEmit",
    "lint": "tslint -c tslint.json './src/**/*.ts'"
  },
  "dependencies": {
    "expo": "^29.0.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "moment": "^2.22.2",
    "immutability-helper": "^2.7.1",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "react-navigation": "^2.9.3"
  },
  "devDependencies": {
    "apsl-react-native-button": "^3.1.1",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "23.5.0",
    "jest-expo": "^29.0.0",
    "react-dom": "^16.3.1",
    "react-test-renderer": "16.3.1"
  },
  "babel": {
    "presets": [
      "babel-preset-expo"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./test/shimSetup.js",
      "./test/jestSetup.ts"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ]
  }
}
